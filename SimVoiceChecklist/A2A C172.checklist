		<item name="Fuel selector valve set to both" vcommand="Set,Checked"/>
		<item name="Fuel shut-off valve pushed in" vcommand="Set,Checked"/>
		<item name="Flaps fully extended" vcommand="Set,Checked,Extended"/>
