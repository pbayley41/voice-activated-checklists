		<item name="Radios and avionics" vcommand="Set,Checked"/>
		<item name="Nav GPS switch" vcommand="Set,Checked"/>
		<item name="Autopilot set off" vcommand="Set,Checked,Off"/>
		<item name="Elevator trim set to take-off" vcommand="Set,Checked"/>
		<item name="Flaps set to take-off" vcommand="Set,Checked"/>
		<item name="Brakes released" vcommand="Set,Checked,Off"/>
	</checklist>
	<checklist name="After Take-off">
		<item name="Climb power" vcommand="Set,Checked"/>
		<item name="Engine gauges in limits" vcommand="Checked"/>
	</checklist>	
	<checklist name="Descent">
		<item name="Power set as required" vcommand="Set,Checked"/>	
		<item name="Mixture adjust for smooth operation" vcommand="Set,Checked"/>			
		<item name="Altimeter" vcommand="Set,Checked"/>		
		<item name="Nav GPS switch" vcommand="Set,Checked"/>
		<item name="Fuel selector valve set to both" vcommand="Set,Checked"/>
		<item name="Flaps set as required" vcommand="Set,Checked"/>
	</checklist>
	<checklist name="Before Landing">
		<item name="Passenger and pilot seat backs upright" vcommand="Set,Checked"/>
		<item name="Seats and seatbelts secure and locked" vcommand="Checked,Yes"/>
		<item name="Fuel selector valve set to both" vcommand="Set,Checked"/>
		<item name="Mixture set to rich" vcommand="Set,Checked"/>
		<item name="Taxi and landing lights set on" vcommand="Set,Checked,On"/>
		<item name="Autopilot set off" vcommand="Set,Checked,Off"/>
		<item name="Flaps set as required" vcommand="Set,Checked"/>		
	</checklist>
	<checklist name="Balked Landing">
		<item name="Mixture set to rich" vcommand="Set,Checked"/>
		<item name="Throttle set to full open" vcommand="Set,Checked"/>
		<item name="Flaps set to 20 degrees" vcommand="Set,Checked"/>		
		<item name="Climb speed 55 knots" vcommand="Checked"/>
	</checklist>
	<checklist name="After Landing">
		<item name="Flaps fully retracted" vcommand="Set,Checked,Retracted"/>
		<item name="Mixture set to lean" vcommand="Set,Checked"/>
		<item name="Strobe lights set off" vcommand="Set,Checked,Off"/>
		<item name="Navigation lights set off" vcommand="Set,Checked,Off"/>
		<item name="Transponder set to standby" vcommand="Set,Checked,Standby"/>
		<item name="Taxi lights set on" vcommand="Set,Checked,On"/>
	</checklist>
	<checklist name="Securing Airplane">
		<item name="Parking brake set on" vcommand="Set,Checked,On"/>
		<item name="Throttle to about 1000rpm" vcommand="Set,Checked"/>
		<item name="Radios set off" vcommand="Set,Checked,Off"/>
		<item name="Autopilot set off" vcommand="Set,Checked,Off"/>
		<item name="Avionics master switch set off" vcommand="Set,Checked,Off"/>
		<item name="Mixture set to idle cut-off" vcommand="Set,Checked"/>
		<item name="Ignition switch set off" vcommand="Set,Checked,Off"/>
		<item name="Taxi and landing lights set off" vcommand="Set,Checked,Off"/>
		<item name="Flashing beacon set off" vcommand="Set,Checked,Off"/>
		<item name="Master switch set off" vcommand="Set,Checked,Off"/>
		<item name="Control wheel lock installed" vcommand="Set,Checked,Installed"/>
		<item name="Tie downs secured" vcommand="Set,Checked,Yes,Secure"/>
		<item name="Wheel chocks in place" vcommand="Set,Checked,Yes"/>
		<item name="Fuel selector valve set to left or right" vcommand="Set,Checked"/>
		<item name="Fuel shut-off valve pulled out" vcommand="Set,Checked"/>
	</checklist>
