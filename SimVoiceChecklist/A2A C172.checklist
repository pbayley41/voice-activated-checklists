<checklists>
	<checklist name="Cabin">
		<item name="Pitot tube cover removed" vcommand="Removed,Checked"/>
		<item name="Pilot's Operating Handbook in airplane" vcommand="Checked,Present,In Airplane"/>
		<item name="Airplane weight and balance checked" instruction="" vcommand="Checked"/>
		<item name="Parking brake set on" vcommand="Set,Checked,On"/>
		<item name="Control wheel lock removed" vcommand="Removed,Checked"/>
		<item name="Ignition switch set off" vcommand="Set,Checked,Off"/>
		<item name="Avionics master switch set off" vcommand="Set,Checked,Off"/>
		<item name="Master switch set on" vcommand="Set,Checked,On"/>
		<item name="Fuel quantity indicators checked" vcommand="Checked"/>
		<item name="Fuel quantity annunciators extinguished" vcommand="Checked,Clear,Extinguished"/>
		<item name="Avionics master switch set on" vcommand="Set,Checked,On"/>
		<item name="Avionics cooling fan audible" vcommand="Checked,Audible,Yes"/>
		<item name="Avionics master switch set off" vcommand="Set,Checked,Off"/>
		<item name="Static pressure alternate source valve set off" vcommand="Set,Checked,Off"/>
