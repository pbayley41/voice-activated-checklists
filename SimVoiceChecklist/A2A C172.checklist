		<item name="Pitot heat set off" vcommand="Set,Checked,Off"/>
		<item name="Master switch set off" vcommand="Set,Off"/>
		<item name="Elevator trim set to take-off" vcommand="Set,Checked"/>
	</checklist>
	<checklist name="Before Starting Engine">
		<item name="Preflight inspection complete?" vcommand="Complete"/>
		<item name="Passenger briefing complete?" vcommand="Complete"/>
		<item name="Seats and seatbelts?" vcommand="Checked"/>
		<item name="Brakes tested and set?" vcommand="Set,Checked"/>
		<item name="Circuit breakers in?" vcommand="Checked"/>
		<item name="Electrical equipment off?" vcommand="Set,Checked,Off"/>
		<item name="Avionics master switch set off" vcommand="Set,Checked,Off"/>
		<item name="Fuel selector valve set to both" vcommand="Set,Checked"/>
		<item name="Fuel shut-off valve pushed in" vcommand="Set,Checked"/>
		<item name="Avionics circuit breakers in?" vcommand="Checked"/>
	</checklist>
	<checklist name="Starting Engine">
		<item name="Throttle quarter inch open" vcommand="Set,Checked"/>
		<item name="Mixture set to idle cut-off" vcommand="Set,Checked"/>
		<item name="Check propeller area clear" vcommand="Checked,Clear"/>
		<item name="Master switch set on" vcommand="Set,Checked,On"/>
		<item name="Flashing beacon set on" vcommand="Set,Checked,On"/>
		<item name="Prime fuel" vcommand="Checked,Primed,Not required"/>
		<item name="Mixture set to idle cut-off" vcommand="Set,Checked,Off"/>
		<item name="Start engine" vcommand="Set,Checked,Started"/>
		<item name="Mixture set to rich" vcommand="Set,Checked"/>
		<item name="Oil pressure" vcommand="Checked"/>
		<item name="Navigation lights set on" vcommand="Set,Checked,On"/>
		<item name="Master switch set off" vcommand="Set,Checked,Off"/>
		<item name="Taxi and landing lights set on" vcommand="Set,Checked,On"/>
		<item name="Reduce engine rpm to idle" vcommand="Set,Checked"/>
		<item name="Master switch set on" vcommand="Set,Checked,On"/>
		<item name="Alternator output below load requirements" vcommand="Checked"/>
		<item name="Increase engine rpm to about 1500" vcommand="Set,Checked"/>
		<item name="Alternator output above load requirements" vcommand="Checked"/>
		<item name="Low volt annunciators extinguished" vcommand="Checked,Clear,Extinguished"/>
		<item name="Avionics master switch set on" vcommand="Set,Checked,On"/>
		<item name="Reduce engine rpm to not below 800" vcommand="Set,Checked"/>
		<item name="Radios set on" vcommand="Set,Checked,On"/>
		<item name="Flaps fully retracted" vcommand="Set,Checked,Retracted"/>
		<item name="Mixture set to lean" vcommand="Set,Checked"/>
	</checklist>
	<checklist name="Before Takeoff">
		<item name="Parking brake set on" vcommand="Set,Checked,On,Off"/>
		<item name="Seats upright" vcommand="Set,Checked"/>
		<item name="Seats and Seatbelts secure" vcommand="Checked"/>
		<item name="Cabin doors closed and locked" vcommand="Set,Checked"/>
		<item name="Flight controls free and correct?" vcommand="Checked"/>
		<item name="Altimeter" vcommand="Set,Checked"/>
		<item name="Fuel quantity" vcommand="Checked"/>
		<item name="Mixture set to rich" vcommand="Set,Checked"/>
		<item name="Fuel selector valve set to both" vcommand="Set,Checked"/>
		<item name="Set throttle to 1800rpm" vcommand="Set"/>
		<item name="Check left magneto operation" vcommand="Checked"/>
		<item name="Check right magneto operation" vcommand="Checked"/>
		<item name="Vacuum gauge within limits" vcommand="Checked"/>
		<item name="Engine instruments and ammeter" vcommand="Checked"/>
		<item name="Annunciator panel clear" vcommand="Checked,Clear"/>
		<item name="Reduce engine rpm to not below 800" vcommand="Set,Checked"/>
